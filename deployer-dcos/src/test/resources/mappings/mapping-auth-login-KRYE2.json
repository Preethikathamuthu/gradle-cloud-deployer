{
  "request": {
    "url": "/acs/api/v1/auth/login",
    "method": "POST",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json; charset=UTF-8"
      }
    },
    "bodyPatterns": [
      {
        "equalToJson": "{\"token\":\"validToken\"}",
        "ignoreArrayOrder": true,
        "ignoreExtraElements": true
      }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "body-auth-login-KRYE2.txt",
    "headers": {
      "Content-Type": "text/plain; charset=utf-8",
      "Connection": "keep-alive"
    }
  }
}