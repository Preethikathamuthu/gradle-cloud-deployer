    "method": "POST",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json; charset=UTF-8"
      }
    },
    "bodyPatterns": [
      {
        "doesNotMatch": "\\{\"token\":\"validToken\"\\}"
      }
    ]
